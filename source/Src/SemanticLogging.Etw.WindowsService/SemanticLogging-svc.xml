<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw SemanticLogging-svc.xsd">
  
  <!-- Optional settings for fine tuning performance and Trace Event Session identification-->
  <traceEventService/>

  <!-- Sinks reference definitons used by this host to listen ETW events -->
  <sinks>
    <!--[Add any built-in or custom sink definition here]-->

    <customSink name="metrics" type="MGS.InfluxDbMetrics.InfluxDbSink, MGS.InfluxDbMetrics">
      <sources>
        <eventSource name="MGS-Metrics" />
      </sources>
      <parameters>
        <parameter name="url" type="System.String" value="http://localhost:8086" />
        <parameter name="databaseName" type="System.String" value="ravi" />
        <parameter name="precision" type="System.String" value="ms" />
        <parameter name="bufferInterval" type="System.Int32" value="10000" />
        <parameter name="bufferingCount" type="System.Int32" value="20" />
        <parameter name="waitWriteTimeInMilliseconds" type="System.Int32" value="100" />
      </parameters>
    </customSink>    

  </sinks>

</configuration>
